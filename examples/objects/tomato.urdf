<?xml version="1.0" ?>
<robot name="tomato.urdf">
  <link name="baseLink">
    <contact>
      <friction_anchor/>
      <lateral_friction value="0.4"/>
      <rolling_friction value="0.002"/>
      <spinning_friction value="0.002"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.2"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="tomato.obj" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!-- <sphere radius="0.01"/> -->
        <mesh filename="tomato.obj" scale="0.01 0.01 0.01"/>
      </geometry>
    </collision>
  </link>
</robot>

